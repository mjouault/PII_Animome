@model Animome.Models.PrerequisNiveau

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p>
    <a asp-action="Index"><ion-icon name="arrow-back"></ion-icon></a>
</p>
<h3>Etes-vous sûr.e de vouloir supprimer le lien entre @Model.Prerequis.Intitule et @Model.Niveau.Intitule ? </h3>
<div>
    <hr />
    <dl class="row">
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger" />
    </form>
</div>
