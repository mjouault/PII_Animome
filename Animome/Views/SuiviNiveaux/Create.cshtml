@model Animome.Models.SuiviNiveau

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>SuiviNiveau</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Etat" /> @Html.DisplayNameFor(model => model.Etat)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="DateValide" class="control-label"></label>
                <input asp-for="DateValide" class="form-control" />
                <span asp-validation-for="DateValide" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

