@model Animome.Models.DomaineUser

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<p>
    <a asp-controller="Parametres" asp-action="Index"><ion-icon name="arrow-back"></ion-icon></a>
</p>

<h3>Etes-vous sûr de vouloir supprimer ce domaine d'expertise pour @Model.ApplicationUser.Prenom @Model.ApplicationUser.Nom ?</h3>
<div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Supprimer" class="btn btn-danger" />
    </form>
</div>
